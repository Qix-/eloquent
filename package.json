{
  "name": "eloquent",
  "description": "Chaining is hard. Make it easy.",
  "version": "1.0.0",
  "author": "Qix (http://github.com/qix-)",
  "keywords": [
    "chain",
    "chaining",
    "chains",
    "getters"
  ],
  "license": "MIT",
  "scripts": {
    "pretest": "node_modules/.bin/xo",
    "test": "node_modules/.bin/mocha --compilers coffee:coffee-script/register"
  },
  "xo": {
    "rules": {
      "no-invalid-this": 0,
      "no-loop-func": 0,
      "no-nested-ternary": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qix-/eloquent.git"
  },
  "devDependencies": {
    "coffee-script": "^1.9.3",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "should": "^7.0.1",
    "xo": "^0.6.1"
  },
  "dependencies": {
    "clone-deep": "^0.2.1",
    "clone-object": "^0.1.0"
  }
}
